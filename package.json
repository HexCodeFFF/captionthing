{
  "name": "aidatasetfromvideo",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/HexCodeFFF/aidatasetfromvideo.git"
  },
  "description": "A utility to sort and slice a captioned video into a dataset for tacotron",
  "main": "index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "melody",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "electron": "^13.1.4"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "electron-builder": "^22.11.7",
    "electron-squirrel-startup": "^1.0.0",
    "open-file-explorer": "^1.0.2",
    "subtitle": "^4.1.0",
    "update-electron-app": "^2.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icon.ico",
        "ignore": [
          "\\.github",
          "\\.idea"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "loadingGif": "loading.gif",
            "setupIcon": "icon.ico",
            "iconUrl": "https://github.com/HexCodeFFF/aidatasetfromvideo/blob/master/icon.ico?raw=true"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "HexCodeFFF",
              "name": "captionthing"
            },
            "prerelease": true
          }
        }
      ]
    }
  }
}
